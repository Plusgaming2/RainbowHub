coroutine.wrap(LBOEAET_fake_script)()
local function MWYKSTK_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local e = 0
	for i, v in pairs(game.Workspace:GetChildren()) do
		if v:IsA("Model") then
			if v:FindFirstChild("Pad") or v:FindFirstChild("Head") then
				e += 20
				script.Parent.CanvasSize = UDim2.new(0,0,0,e)
				local button = Instance.new("TextButton", script.Parent)
				button.Name = v.Name
				button.Text = v.Name
				button.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
				button.TextColor3 = Color3.fromRGB(0, 0, 0)
				button.TextScaled = true
				button.MouseButton1Click:Connect(function()
					firetouchinterest(game.Players.LocalPlayer.Character.Head, v:FindFirstChildOfClass("Part"), 0)
					wait()
					firetouchinterest(game.Players.LocalPlayer.Character.Head, v:FindFirstChildOfClass("Part"), 1)
				end)
			end
		end
	end
end
coroutine.wrap(MWYKSTK_fake_script)()
local function RCKRU_fake_script() -- Search.LocalScript 
	local script = Instance.new('LocalScript', Search)

	script.Parent.Changed:Connect(function(type)
		if type == "Text" then
			for _, v in pairs(script.Parent:GetChildren()) do
				if v:IsA("TextButton") then
					v.Parent = script.Parent.ScrollingFrame
					v.Visible = true
				end
			end
			for i, v in pairs(script.Parent.ScrollingFrame:GetChildren()) do
				if v:IsA("TextButton") then
					if not string.match(string.lower(v.Name), string.lower(script.Parent.Text)) then
						v.Parent = script.Parent
						v.Visible = false
					end
				end
			end
		end
	end)
end
coroutine.wrap(RCKRU_fake_script)()
local function IRYEKWT_fake_script() -- Main_4.LocalScript 
	local script = Instance.new('LocalScript', Main_4)

	local function lerp(a, b, t)
		return a + (b - a) * t
	end
	script.Parent.Changed:Connect(function()
		script.Parent.Size = UDim2.new(script.Parent.Size.X.Scale,script.Parent.Size.X.Offset,0,script.Parent.AbsoluteSize.X)
	end)
	script.Parent.MouseButton1Click:Connect(function()
		for i, v in pairs(script.Parent.Parent.Parent.Parent:GetDescendants()) do
			if (v.Name ~= "Main") and (v:IsA("GuiBase")) then
				v:Destroy()
			end
		end
		for i = 0,1,0.05 do
			wait(0.075)
			script.Parent.Parent.Parent.Parent.Size = script.Parent.Parent.Parent.Parent.Size:Lerp(UDim2.new(0.061, 0,0.123, 0), i)
			script.Parent.Parent.Size = script.Parent.Parent.Size:Lerp(UDim2.new(1,0,1,0), i)
		end
		wait(1)
		for i = 0,1,0.05 do
			wait(0.075)
			script.Parent.Parent.Parent.Parent.Size = script.Parent.Parent.Parent.Parent.Size:Lerp(UDim2.new(0, 0,0, 0), i)
		end
		script.Parent.Parent.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(IRYEKWT_fake_script)()
