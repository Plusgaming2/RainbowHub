local function SNFYIX_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		firetouchinterest(game.Players.LocalPlayer.Character.Head, game.Workspace:FindFirstChild("Morph remover").Button, 0)
		wait()
		firetouchinterest(game.Players.LocalPlayer.Character.Head, game.Workspace:FindFirstChild("Morph remover").Button, 1)
	end)
end
coroutine.wrap(SNFYIX_fake_script)()
