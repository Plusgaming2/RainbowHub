local function OKMZL_fake_script() -- Main.Drag 
	local script = Instance.new('LocalScript', Main)

	local Lerp = function(a, b, t)
		return a + (b - a) * t
	end
	local Drag = function(Click, Drag, Smoothness)
		local plr = game.Players.LocalPlayer
		local m = plr:GetMouse()
		local clicked = false
		local mx = m.X
		local my = m.Y
		local wantedpos = Vector2.new(Drag.Position.X.Offset, Drag.Position.Y.Offset)
		if Click:IsA("GuiButton") then
			Click.MouseButton1Down:Connect(function()
				if ((m.X > Click.AbsolutePosition.X) and (m.Y > Click.AbsolutePosition.Y)) and ((m.X < (Click.AbsolutePosition.X + Click.AbsoluteSize.X)) and (m.Y < (Click.AbsolutePosition.Y + Click.AbsoluteSize.Y))) then
					clicked = true
					mx = m.X
					my = m.Y
					wantedpos = Vector2.new(Drag.Position.X.Offset, Drag.Position.Y.Offset)
				end
			end)
			Click.MouseButton1Up:Connect(function()
				clicked = false
			end)
			m.Button1Up:Connect(function()
				clicked = false
			end)
			game:GetService("RunService").RenderStepped:Connect(function(dt)
				if clicked then
					wantedpos = Vector2.new(wantedpos.X - (m.X - mx), wantedpos.Y - (m.Y - my))
					Drag.Position = UDim2.new(Drag.Position.X.Scale,Lerp(Drag.Position.X.Offset, wantedpos.X, Smoothness),Drag.Position.Y.Scale,Lerp(Drag.Position.Y.Offset, wantedpos.Y, Smoothness))
					mx = m.X
					my = m.Y
				end
			end)
		else
			m.Button1Down:Connect(function()
				if ((m.X > Click.AbsolutePosition.X) and (m.Y > Click.AbsolutePosition.Y)) and ((m.X < (Click.AbsolutePosition.X + Click.AbsoluteSize.X)) and (m.Y < (Click.AbsolutePosition.Y + Click.AbsoluteSize.Y))) then
					clicked = true
					mx = m.X
					my = m.Y
					wantedpos = Vector2.new(Drag.Position.X.Offset, Drag.Position.Y.Offset)
				end
			end)
			m.Button1Up:Connect(function()
				clicked = false
			end)
			game:GetService("RunService").RenderStepped:Connect(function(dt)
				if clicked then
					wantedpos = Vector2.new(wantedpos.X - (mx - m.X), wantedpos.Y - (my - m.Y))
					Drag.Position = UDim2.new(Drag.Position.X.Scale,Lerp(Drag.Position.X.Offset, wantedpos.X, Smoothness),Drag.Position.Y.Scale,Lerp(Drag.Position.Y.Offset, wantedpos.Y, Smoothness))
					mx = m.X
					my = m.Y
				end
			end)
		end
	end
	
	Drag(script.Parent,script.Parent, 0.15)
end
coroutine.wrap(OKMZL_fake_script)()
