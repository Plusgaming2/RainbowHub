-- Scripts:

local function DDMZP_fake_script() -- Home.RainbowGui 
	local script = Instance.new('LocalScript', Home)

	local e = 0.1
	game:GetService("RunService").RenderStepped:Connect(function(dt)
		e += 0.025 * (dt * 10)
		script.Parent.BackgroundColor3 = Color3.fromHSV(e, 1, 1)
		if e >= 1 then
			script.Parent.BackgroundColor3 = Color3.fromHSV(0,1,1)
			e = 0
		end
	end)
end
coroutine.wrap(DDMZP_fake_script)()
local function ZBFRUYX_fake_script() -- TextLabel.RainbowGui 
	local script = Instance.new('LocalScript', TextLabel)

	local e = 0.1
	script.Parent.Text = "Welcome to Rainbow Hub, ".. game.Players.LocalPlayer.DisplayName
	game:GetService("RunService").RenderStepped:Connect(function(dt)
		e += 0.025 * (dt * 10)
		script.Parent.TextColor3 = Color3.fromHSV(e, 1, 1)
		if e >= 1 then
			script.Parent.TextColor3 = Color3.fromHSV(0,1,1)
			e = 0
		end
	end)
end
coroutine.wrap(ZBFRUYX_fake_script)()
local function RZIE_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	local plr = game.Players.LocalPlayer
	
	script.Parent.BackgroundTransparency = 1
	script.Parent.Image = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
end
coroutine.wrap(RZIE_fake_script)()
local function TPKMLHQ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local current = script.Parent.Home
	
	local lerping = false
	
	local function Lerp(a, b, c)
		return a + (b - a) * c
	end
	
	for i, v in pairs(script.Parent:GetChildren()) do
		if v:IsA("TextButton") then
			v.MouseButton1Click:Connect(function()
				if not lerping then
					if v ~= current then
						lerping = true
						v.Frame.AnchorPoint = Vector2.new(1,0)
						v.Frame.Visible = true
						current.Frame.AnchorPoint = Vector2.new(0,0)
						spawn(function()
							for i = 0,1,0.01 do
								current.Frame.AnchorPoint = Vector2.new(Lerp(current.Frame.AnchorPoint.X, 1, i), Lerp(current.Frame.AnchorPoint.Y, 0, i))
								wait(0.02)
							end
						end)
						for i = 0,1,0.01 do
							v.Frame.AnchorPoint = Vector2.new(Lerp(v.Frame.AnchorPoint.X, 0, i), Lerp(v.Frame.AnchorPoint.Y, 0, i))
							wait(0.02)
						end
						current.Frame.Visible = false
						current = v
						lerping = false
					end
				end
			end)
		end
	end
end
coroutine.wrap(TPKMLHQ_fake_script)()
