local function EPLEBDC_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local Stuff = require(script.Parent.Scripts)
	
	for _, v in pairs(Stuff) do
		local button = Instance.new("TextButton", script.Parent)
		button.BackgroundColor3 = Color3.fromRGB(255,0,0)
		button.BorderSizePixel = 0
		button.Text = v["Name"]
		button.TextScaled = true
		button.TextWrapped = true
		button.Name = v["Name"]
		button.MouseButton1Click:Connect(function()
			loadstring(game:HttpGet(v["Script"], true))()
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent:Destroy()
		end)
	end
end
coroutine.wrap(EPLEBDC_fake_script)()
local function VLJW_fake_script() -- Search.LocalScript 
	local script = Instance.new('LocalScript', Search)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.Changed:Connect(function(type)
		if type == "Text" then
			for i, v in pairs(script.Parent:GetChildren()) do
				if v:IsA("TextButton") then
					v.Parent = script.Parent.ScrollingFrame
					v.Visible = true
				end
			end
			for i, v in pairs(script.Parent.ScrollingFrame:GetChildren()) do
				if not string.match(string.lower(v.Name), string.lower(script.Parent.Text)) then
					v.Parent = script.Parent
					v.Visible = false
				end
			end
		end
	end)
end
coroutine.wrap(VLJW_fake_script)()
local function VUYL_fake_script() -- Main_4.LocalScript 
	local script = Instance.new('LocalScript', Main_4)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local function lerp(a, b, t)
		return a + (b - a) * t
	end
	script.Parent.Changed:Connect(function()
		script.Parent.Size = UDim2.new(script.Parent.Size.X.Scale,script.Parent.Size.X.Offset,0,script.Parent.AbsoluteSize.X)
	end)
	script.Parent.MouseButton1Click:Connect(function()
		for i, v in pairs(script.Parent.Parent.Parent.Parent:GetDescendants()) do
			if (v.Name ~= "Main") and (v:IsA("GuiBase")) then
				v:Destroy()
			end
		end
		for i = 0,1,0.05 do
			wait(0.075)
			script.Parent.Parent.Parent.Parent.Size = script.Parent.Parent.Parent.Parent.Size:Lerp(UDim2.new(0.061, 0,0.123, 0), i)
			script.Parent.Parent.Size = script.Parent.Parent.Size:Lerp(UDim2.new(1,0,1,0), i)
		end
		wait(1)
		for i = 0,1,0.05 do
			wait(0.075)
			script.Parent.Parent.Parent.Parent.Size = script.Parent.Parent.Parent.Parent.Size:Lerp(UDim2.new(0, 0,0, 0), i)
		end
		script.Parent.Parent.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(VUYL_fake_script)()
