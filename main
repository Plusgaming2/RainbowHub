-- Module Scripts:

local fake_module_scripts = {}

do -- ScrollingFrame.Scripts
	local script = Instance.new('ModuleScript', ScrollingFrame)
	script.Name = "Scripts"
	local function module_script()
		local module = {
			[1] = {
				Name = "Animatronic World !";
				Script = 'https://raw.githubusercontent.com/Plusgaming2/RainbowHub/scripts/Animatronic%20World%20!'
			}
		}
		
		return module
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function CRGMREL_fake_script() -- Home.RainbowGui 
	local script = Instance.new('LocalScript', Home)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local e = 0.1
	game:GetService("RunService").RenderStepped:Connect(function(dt)
		pcall(function()
			e += 0.025 * (dt * 10)
			script.Parent.BackgroundColor3 = Color3.fromHSV(e, 1, 1)
			if e >= 1 then
				script.Parent.BackgroundColor3 = Color3.fromHSV(0,1,1)
				e = 0
			end
		end)
	end)
end
coroutine.wrap(CRGMREL_fake_script)()
